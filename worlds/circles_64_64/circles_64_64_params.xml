<?xml version="1.0"?>
<opencv_storage>

	<!-- Agent Stuff -->
	<num_agents>3</num_agents>
	<num_types>2</num_types>
	<agent_types type_id="opencv-matrix">
  		<rows>3</rows> <!-- Agent -->
  		<cols>1</cols> <!-- Types -->
  		<dt>i</dt>
  		<data>0 1 0</data>
  	</agent_types>
	<agent_starts type_id="opencv-matrix">
  		<rows>3</rows> <!-- Agent -->
  		<cols>2</cols> <!-- Loc XY -->
  		<dt>i</dt>
  		<data>5 5
  		 10 10
  		25 25</data>
  	</agent_starts>
	<agent_work type_id="opencv-matrix">
  		<rows>2</rows> <!-- Agent Types-->
  		<cols>2</cols> <!-- Task Types-->
  		<dt>i</dt>
  		<data>1 2
  		 3 4</data>
  	</agent_work>
	
	<!-- Costmap Stuff -->
	<cells_per_pixel>1.0</cells_per_pixel>
	<cells_width>1377</cells_width>
	<cells_height>817</cells_height>
	<inflation_threshold>50</inflation_threshold>

	<!-- PRM Stuff -->
	<n_nodes>5</n_nodes>
	<p_task_init_active>0.5</p_task_init_active>

	<!-- Task Stuff -->
	<min_work>10</min_work>
	<max_work>100</max_work>
	<work_radius>500</work_radius>


</opencv_storage>